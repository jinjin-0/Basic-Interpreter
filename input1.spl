function f(int a)        // 함수 f(int a) 정의
begin
   int b = 6;            // 정수 변수 b를 6으로 초기화
   int c = 2;            // 정수 변수 c를 2으로 초기화
   ((b+c)/a);            // (b + c) / a 수식 계산
end

function main()          // 함수 main() 정의
begin
   int a = 1;            // 정수 변수 a를 1로 초기화
   int b = 2;            // 정수 변수 b를 2로 초기화
   int c = 4;            // 정수 변수 c를 4로 초기화
   ((6 + f(c) ) / b);    // (6 + f(c)) / b 수식 계산
end
